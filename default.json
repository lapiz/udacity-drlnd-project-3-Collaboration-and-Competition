{
    "output": "default",
    "seed": 0,
    "epoch": 5000,
    "scores": {
        "expectation": 0.5,
        "window_size": 100,
        "check_solved": true
    },
    "t_max": 100,
    "buffer_size":10000,
    "batch_size":64,
    "gamma":0.99,
    "tau":1e-3,
    "lr": { 
        "actor":1e-4,
        "critic":1e-4
    },
    "weight_decay": 0,

    "learn_per_step": 1,
    "update_times": 1,
    
    "hidden_layers": {
        "actor": [ 256, 128 ],
        "critic": [ 256, 128 ]
    }
}
